	background-color: #28292e;
	border-color: #687df3;
}

#line_6 {
	background-color: #28292e;
	color: #687df3;
}

#line_7 {
	background-color: #28292e;
	color: #687df3;
}

#line_3 {
	background-color: #4c4959;
	color: #687df3;
}

#line_2 {
	background-color: #4c4959;
	color: #687df3;
}